<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>산술 연산자</title>
    <script>
        let num1 = 25;
        let num2 = 4;
        let resert;
        console.log(num1);
        console.log(num2);
        console.log(resert);

        resert = num1 + num2; //
        document.write(resert,'<br>');
        resert = num1 - num2; //
        document.write(resert,'<br>');
        resert = num1 * num2; // 100
        document.write(resert,'<br>');
        resert = num1 / num2; // 6.25
        document.write(resert,'<br>');
        resert = num1 % num2; //1
        document.write(resert,'<br><br>');

        num1++; // 26
        document.write(num1 + '<br>'); // 연결자 +
        resert = num1++; //resert? 26, num? 27
        document.write(resert + ' '+ num1 +'<br>');
        resert2 = ++num2; // num2 -> 4 resert2? 5, num2? 5
        document.write(resert2 + ' ' +num2);
    </script>
</head>
<body>
    
</body>
</html>
