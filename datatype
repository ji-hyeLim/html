<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>데이터 타입</title>
    <script>
        // number
        var num1 = 10;
        var num2 = 10.5;
        console.log(num1+num2);

        // string
        var str1 = '문자';
        var str2 = 'you\'re too smart!'
        console.log(str2);

        // boolean
        var com1 = 5 > 4; // true
        var com2 = 5 < 4; // false
        console.log(com1 + ',' + com2);

        // undefined
        var result;
        console.log(result);

        // null
        var title = document.getElementById('title');
        console.log(title);

        // typeof
        var type1 = 10;
        var type2 = "텍스트";
        var type3 = true;
        console.log(typeof type1);
        console.log(typeof type2);
        console.log(typeof type3);

    </script>
</head>
<body>
    
</body>
</html>
