    <title>const 알아보기</title>
    <script>
        const DIVISION_VALUE = 100;
        console.log(DIVISION_VALUE);

        const cm = Number(prompt("cm를 입력하세요!","160")); // 변수에 대입하기
        const result = cm / DIVISION_VALUE;
        document.write(result + '입니다.'); // +는 연결자

    </script>
